{
    "name": "rpi_temperature",
    "private": true,
    "version": "0.1.0",
    "dependencies": {
        "@types/axios": "^0.14.0",
        "axios": "^0.18.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-scripts-ts": "2.16.0",
        "ts-jest": "^23.0.1"
    },
    "devDependencies": {
        "@types/jest": "^23.3.0",
        "@types/node": "^10.5.2",
        "@types/react": "^16.4.6",
        "@types/react-dom": "^16.0.6",
        "lint-staged": "^7.2.0",
        "prettier": "^1.13.7",
        "typescript": "^2.9.2"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "prettier --write",
            "tslint --fix",
            "git add"
        ]
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "proxy": "http://localhost:3001",
    "scripts": {
        "build": "react-scripts-ts build",
        "eject": "react-scripts-ts eject",
        "precommit": "lint-staged",
        "prettier": [
            "prettier --single-quote --trailing-comma es6 --write"
        ],
        "start": "react-scripts-ts start",
        "test": "react-scripts-ts test --env=jsdom"
    }
}
