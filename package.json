{
    "name": "myas_temp",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "author": "Narin Sundarabhaya",
    "scripts": {
        "server": "nodemon -w server/server.js --exec \"babel-node server/server.js --presets env,es2015,stage-2\"",
        "start": "node server/server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "client": "cd client && yarn start",
        "prettier": "prettier --single-quote --trailing-comma es5 --write",
        "precommit": "lint-staged",
        "build": "babel server -s -D -d dist --presets env,es2015,stage-2"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "dependencies": {
        "body-parser": "^1.18.3",
        "concurrently": "^3.6.0",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "dotenv": "^6.0.0",
        "eslint": "^5.1.0",
        "express": "^4.16.3",
        "mongodb": "^3.1.1",
        "mongoose": "^5.2.3",
        "morgan": "^1.9.0",
        "node-cron": "^1.2.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "husky": "^0.14.3",
        "lint-staged": "^7.2.0",
        "prettier": "^1.13.7"
    }
}
